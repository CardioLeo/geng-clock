# Description: A minimal test run of my geng clock application
# Usage: geng-clock

#!/bin/bash

# x=1; while [ $x -le 5 ]; do echo "Welcome $x times"; x=$(( $x + 1 )); done

# geng=8640;

geng=5;
i=0;

echo "geng-clock started";

while [ $i -le $geng ];
do
	echo $i
	sleep 1
	i=$(( $i + 1 ))
done
echo "geng-clock process over"
